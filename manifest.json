{
  "name": "Aleatory",
  "version": "0.1.3",
  "manifest_version": 2,
  "description": "Listen to your browser make requests.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "src/popup/popup.html"
  },
  "icons": {
    "16": "icon.png"
  },
  "background": {
    "scripts": [
      "src/background/util.js",
      "src/background/functions.js",
      "src/background/charlie.js"
    ]
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "*://*/*"
  ]
}
